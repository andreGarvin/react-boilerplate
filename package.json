  },
  "author": "Andre Garvin <Yea Boi>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-react": "7.0.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "6.3.2",
    "css-loader": "2.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "style-loader": "0.23.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreGarvin/react-boilerplate.git"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.8.1"
