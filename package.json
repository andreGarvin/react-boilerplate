{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "This is a simple react boilerplate application starter code using webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production --colors --display-error-details --config webpack.config.js",
