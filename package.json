{
  "name": "react-bolierplate-setup",
  "version": "0.0.0",
  "description": "This is a simple react bolierplate appllication using webpack for bundling and babel for trans-compilation",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p && cp App/index.html dist/index.html",
    "build:dev": "webpack -d && cp App/index.html dist && webpack-dev-server --content-base app/ --inline --hot"
  },
  "author": "Andre Garvin <Yea Boi>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.7",
    "style-loader": "^0.19.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6",
    "webpack-dev-server": "^3.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreGarvin/react-boilerplate-setup.git"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
